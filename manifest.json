{
  "manifest_version": 2,
  "name": "Enable Design Mode",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon-no-edit.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}